{
    "name": "@sigauth/generics",
    "version": "0.1.5",
    "type": "module",
    "description": "SigAuth Generic Types for the core integration",
    "keywords": [
        "sigauth",
        "core",
        "authentication",
        "authorization",
        "types"
    ],
    "bugs": {
        "url": "https://github.com/SigAuth/core/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SigAuth/core.git"
    },
    "scripts": {
        "build": "tsc"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/extended-prisma.js",
    "types": "./dist/extended-prisma.d.ts",
    "exports": {
        ".": {
            "types": "./dist/extended-prisma.d.ts",
            "import": "./dist/extended-prisma.js",
            "default": "./dist/extended-prisma.js"
        },
        "./prisma-extended": {
            "types": "./dist/extended-prisma.d.ts",
            "import": "./dist/extended-prisma.js",
            "default": "./dist/extended-prisma.js"
        },
        "./prisma-client": {
            "types": "./dist/prisma-generated/client.d.ts",
            "import": "./dist/prisma-generated/client.js",
            "default": "./dist/prisma-generated/client.js"
        },
        "./prisma-types": {
            "types": "./dist/prisma-generated/client.d.ts",
            "default": "./dist/empty.js"
        },
        "./json-types": {
            "types": "./dist/json-types.d.ts",
            "import": "./dist/json-types.js",
            "default": "./dist/json-types.js"
        },
        "./protected": {
            "types": "./dist/protected.d.ts",
            "import": "./dist/protected.js",
            "default": "./dist/protected.js"
        }
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.1.0",
        "@prisma/client": "^7.1.0",
        "dotenv": "^17.2.3"
    },
    "devDependencies": {
        "prisma": "^7.1.0",
        "typescript": "latest"
    }
}

