{
    "name": "@sigauth/prisma-wrapper",
    "version": "0.0.1",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc"
    },
    "files": [
        "dist"
    ],
    "exports": {
        "./prisma-extended": {
            "import": "./dist/extended-prisma.js",
            "types": "./dist/extended-prisma.d.ts",
            "default": "./dist/extended-prisma.js"
        },
        "./prisma-client": {
            "import": "./dist/prisma-generated/client.js",
            "types": "./dist/prisma-generated/client.d.ts",
            "default": "./dist/prisma-generated/client.js"
        },
        "./prisma-types": {
            "types": "./dist/prisma-generated/client.d.ts",
            "default": "./dist/empty.js"
        },
        "./json-types": {
            "import": "./dist/json-types.js",
            "types": "./dist/json-types.d.ts",
            "default": "./dist/json-types.js"
        },
        "./protected": {
            "import": "./dist/protected.js",
            "types": "./dist/protected.d.ts",
            "default": "./dist/protected.js"
        }
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.1.0",
        "@prisma/client": "^7.1.0",
        "dotenv": "^17.2.3"
    },
    "devDependencies": {
        "prisma": "^7.1.0",
        "typescript": "latest"
    }
}
