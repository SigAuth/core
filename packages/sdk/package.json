{
    "name": "@sigauth/sdk",
    "version": "0.1.0",
    "type": "module",
    "description": "SigAuth SDK for auth, generic types and managing asset types and their assets.",
    "keywords": [
        "sigauth",
        "core",
        "authentication",
        "authorization",
        "types",
        "sdk"
    ],
    "bugs": {
        "url": "https://github.com/SigAuth/core/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SigAuth/core.git"
    },
    "scripts": {
        "build": "tsc"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/extended-prisma.js",
    "types": "./dist/extended-prisma.d.ts",
    "exports": {
        "./protected": {
            "types": "./dist/protected.types.d.ts",
            "import": "./dist/protected.types.js",
            "default": "./dist/protected.types.js"
        },
        "./mirror": {
            "types": "./dist/mirror.types.d.ts",
            "import": "./dist/mirror.types.js",
            "default": "./dist/mirror.types.js"
        },
        "./asset": {
            "types": "./dist/asset.types.d.ts",
            "import": "./dist/asset.types.js",
            "default": "./dist/asset.types.js"
        }
    },
    "dependencies": {
        "dotenv": "^17.2.3"
    },
    "devDependencies": {
        "typescript": "latest"
    }
}

